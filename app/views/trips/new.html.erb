<h2>Create a New Trip</h2>

<%= form_for @trip do |f| %>

  <%= f.label :trip_nickname %>
  <%= f.text_field :trip_nickname %><br>

  <%= f.label :start_date %>
  <%= f.text_field :start_date %><br>

  <%= f.label :end_date %>
  <%= f.text_field :end_date %><br>

  <%= f.label :destination_id %>
  <%= f.text_field :destination_id %><br>

  <%= f.submit "Create This Trip" %>

<% end %>
